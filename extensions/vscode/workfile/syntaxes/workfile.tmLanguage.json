{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Workfile",
	"patterns": [
		{
			"include": "#project"
		}
	],
	"repository": {
		"project": {
			"match": "^(#)\\s+(.*?)(\\/\\s*(.*)).$",
			"captures": {
				"1": {
					"patterns": [
						{
							"match": "^(#)\\s+(.*?)(\\/\\s*(.*)).$",
							"name": "namespace.workfile",
							"captures": {
								"1": {
									"name": "token.definition.namespace.workfile"
								},
								"2": {
									"name": "entity.name.namespace.workfile",
									"patterns": [
										{
											"include": "#inline"
										},
										{
											"include": "text.html.derivative"
										}
									]
								}
							}
						}
					]
				}
			},
			"name": "markup.namespace.workfile",
			"patterns": [
				{
					"include": "#inline"
				}
			]
		}
	},
	"scopeName": "text.htmls.markdown"
}